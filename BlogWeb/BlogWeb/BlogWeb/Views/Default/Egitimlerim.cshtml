@using BlogWeb.Models.Entity
@model List<TBLEGITIM>

<div class="my-auto">
    <h2 class="mb-5">EĞİTİM HAYATIM</h2>
    @foreach (var e in Model)
    {
        <div class="resume-item d-flex flex-column flex-md-row mb-5">

            <div class="resume-content mr-auto">
                <h3 class="mb-0">@e.BASLIK</h3>
                <div class="subheading mb-3">@e.ALTBASLIK</div>
                <div>@e.ACIKLAMA</div>
                <p>@e.NOT</p>
            </div>
            <div class="resume-date text-md-right">
                <span class="text-primary">@e.TARIH</span>
            </div>
        </div>
    }

      

    </div>
