@using BlogWeb.Models.Entity
@model List<BlogWeb.Models.Entity.TBLPROJELER>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Panel.cshtml";
}


<table class="table table-striped" style="margin-top:100px;">

    <tr style="background-color:rgb(252,178,22)">
        <th>Proje Id</th>
        <th>Proje Adı</th>
        <th>Proje Linki</th>
        <th>Sil</th>
        <th>Güncelle</th>
    </tr>
    @foreach (var proje in Model)
    {
        <tr>
            <td>@proje.ID</td>
            <td>@proje.AD</td>
            <td>@proje.LİNK</td>
            <td><a href="/Projelerim/ProjeSil/@proje.ID" class="btn btn-danger" onclick="return sor()">Sil</a></td>
            <td><a href="/Projelerim/ProjeGetir/@proje.ID" class="btn btn-success">Güncelle</a></td>
        </tr>
    }
</table>

