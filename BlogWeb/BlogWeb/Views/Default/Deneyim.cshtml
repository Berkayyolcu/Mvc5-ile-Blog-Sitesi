@using BlogWeb.Models.Entity
@model List<TBLDENEYIM>


<div class="my-auto">
    <h2 class="mb-5">DENEYİMİM</h2>

    @foreach (var dn in Model)
    {

        <div class="resume-item d-flex flex-column flex-md-row mb-5">
            <div class="resume-content mr-auto">
                <h3 class="mb-0">@dn.BASLIK</h3>
                <div class="subheading mb-3">@dn.ALTBASLIK</div>
                <p>@dn.ACIKLAMA</p>
            </div>
            <div class="resume-date text-md-right">
                <span class="text-primary">@dn.TARIH</span>
            </div>
        </div>
    }
    </div>


