<div class="my-auto">
    <h2 class="mb-5">SERTİFİKALARIM</h2>
    <ul class="fa-ul mb-0">
        @foreach (var s in Model)
        {
            <li>
                <i class="fa fa-certificate"></i>
                @s.SERTIFIKALARIM
            </li>

        }
        </ul>
</div>
