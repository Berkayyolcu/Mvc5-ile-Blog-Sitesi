@model BlogWeb.Models.Entity.TBLMETATAG
@{
    ViewBag.Title = "MetaEkle";
    Layout = "~/Views/Shared/Panel.cshtml";
}


<div class="container mt-5">

    <form class="form-group" method="post">
        <div class="row mt-5">

            <h3 class="text-center text-danger">Meta Etiketleri Ekleme</h3>

            <input type="text" class="form-control mt-2" name="Abstract" placeholder="Site özeti" />
            <input type="text" class="form-control mt-2" name="Author" placeholder="Site Yapımcısının Adı ve İletisim Bilgileri" />
            <input type="text" class="form-control mt-2" name="Description" placeholder="Site İceriği Acıklama" />
            <input type="text" class="form-control mt-2" name="Keywords" placeholder="Anahtar Kelimeler" />
            <input type="text" class="form-control mt-2" name="Title" placeholder="Site Baslık" />

        </div>
        <button class="btn btn-primary mt-2" id="Btn1">Meta Etiketi Ekle</button>
    </form>

</div>