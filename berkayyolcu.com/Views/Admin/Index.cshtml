@using BlogWeb.Models.Entity
@model List<BlogWeb.Models.Entity.TBLADMİN>

@{
    ViewBag.Title = "Detay";
    Layout = "~/Views/Shared/Panel.cshtml";
}



<table class="table table-striped" style="margin-top:100px;">
    <tr style="background-color:rgb(252,178,22)">
        <th>Kullanıcı Id</th>
        <th>Kullanıcı Adı</th>
        <th>Sil</th>
        <th>Güncelle</th>
    </tr>
    @foreach (var kullanıcı in Model)
    {
        <tr>
            <td>@kullanıcı.ID</td>
            <td>@kullanıcı.User</td>
            <td><a href="/Admin/AdminSil/@kullanıcı.ID" class="btn btn-danger" onclick="return sor()">Sil</a></td>
            <td><a href="/Admin/AdminGetir/@kullanıcı.ID" class="btn btn-success">Güncelle</a></td>
        </tr>
    }
</table>

